#include <iostream>
#include <cmath>
using namespace std;
int main(){
    double x1, y1, r1, x2, y2, r2;
    double r;
    cin>>x1>>y1>>r1>>x2>>y2>>r2;
    r = sqrt((x2-x1)*(x2-x1) + (y2-y1)*(y2-y1));
    if((r+r1>=r2) && (r+r2>=r1) && (r2+r1>=r)){
        cout<<"YES";
    }
    else{
        cout<<"NO";
    }
}
